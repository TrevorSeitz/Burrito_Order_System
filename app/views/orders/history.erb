<h1>ORDER HISTORY</h1>
<% Order.all.each do |order|%>
<%# binding.pry %>
<% if order.user_id == current_user.id%>
  <p><a href="/orders/<%= order.id %>">Order #: <%= order.id %></a> Store:  <%= @store.store_name %> <%= order.order_total %><br>
  Ordered by: <%= @user.name %></p> 
<%end%><br>
<%# binding. pry %>
<%end%>


